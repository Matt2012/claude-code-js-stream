{
  "name": "claude-code-js",
  "version": "0.3.1",
  "description": "A TypeScript SDK wrapper for claude-code CLI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "claude",
    "claude-code",
    "sdk",
    "cli",
    "wrapper",
    "anthropic"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s-soroosh/claude-code-js.git"
  },
  "bugs": {
    "url": "https://github.com/s-soroosh/claude-code-js/issues"
  },
  "homepage": "https://github.com/s-soroosh/claude-code-js#readme",
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.3",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "execa": "^9.5.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
